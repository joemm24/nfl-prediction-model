# Example secrets file for Streamlit Cloud
# Copy this to secrets.toml and fill in your values
# NEVER commit secrets.toml to git!

# User Authentication (REQUIRED)
# Passwords should be SHA256 hashed
# To generate: python -c "import hashlib; print(hashlib.sha256('yourpassword'.encode()).hexdigest())"
[users]
demo = "b5ac92bdf23aeb7c7f9be1b8267387fe148be7e6b578b79bc3287c933c2450fe"  # Password: "nfl2025"
# admin = "your_hashed_password_here"
# user1 = "your_hashed_password_here"

# Example: AWS credentials (if using S3 for model storage)
# [aws]
# access_key_id = "YOUR_ACCESS_KEY"
# secret_access_key = "YOUR_SECRET_KEY"
# region = "us-east-1"
# bucket_name = "your-model-bucket"

# Example: Database connection (if adding persistence)
# [database]
# host = "your-db-host.com"
# port = 5432
# name = "nfl_predictions"
# user = "db_user"
# password = "db_password"

# Example: API keys (if integrating external services)
# [api]
# sportsdata_key = "your-api-key"
# sendgrid_key = "your-sendgrid-key"

